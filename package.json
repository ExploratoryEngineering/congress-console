{
  "name": "lora-self-service-portal",
  "version": "0.19.0",
  "description": "Front end for self service portal for Exploratory Engineering LORA",
  "main": "dist",
  "productName": "LORA Self Service Portal",
  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "cross-env NODE_ENV=development npm run webpack -- --progress --profile",
    "build:prod": "webpack -p --env.production --env.extractCss --verbose false",
    "clean": "npm cache clean && del-cli node_modules \"test/coverage\" dist",
    "clean:dist": "del-cli dist",
    "clean:install": "npm install",
    "clean:start": "npm start",
    "commitmsg": "validate-commit-msg",
    "deploy:pi": "npm run build:prod && ./scripts/deployToPi.sh",
    "deploy:staging": "npm run test && npm run build:prod && ./scripts/deployToStaging.sh",
    "e2e": "concurrently --success first --kill-others \"npm run e2e:start-when-ready\" \"cross-env npm run e2e:start-server\"",
    "e2e:start-server": "npm run build:prod && http-server dist --cors -p 19876 --silent",
    "e2e:start-when-ready": "wait-on --timeout 120000 http-get://localhost:19876/index.html && npm run e2e:start",
    "e2e:start": "protractor test/protractor.conf.js",
    "e2e:live": "npm run e2e:start -- --elementExplorer",
    "preclean:start": "npm run clean",
    "pree2e": "npm run webdriver:update -- --standalone",
    "prebuild:dev": "npm run clean:dist",
    "prebuild:prod": "npm run clean:dist",
    "preclean:install": "npm run clean",
    "release": "standard-version --silent",
    "release:patch": "npm run release -- --release-as patch",
    "release:minor": "npm run release -- --release-as minor",
    "release:major": "npm run release -- --release-as major",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server -d --devtool '#source-map' --inline --env.server --env.extractCss",
    "server:dev:hmr": "npm run server:dev -- --hot",
    "server:prod": "npm run build:prod && http-server dist --cors -p 8080 --silent",
    "start": "npm run server:dev",
    "test": "npm run test:jest && npm run test:lint && npm run test:retire && npm run e2e",
    "test:jest": "del-cli test/coverage-jest && jest --config jest.config.json",
    "test:eslint": "eslint src test/e2e test/unit",
    "test:lint": "npm run test:eslint && npm run test:tslint && npm run test:lesshint",
    "test:lesshint": "./node_modules/lesshint/bin/lesshint styles",
    "test:tslint": "tslint src/**/*.ts && tslint test/**/*.ts",
    "test:retire": "retire .",
    "test:debug": "npm test -- --single-run=false --debug",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:test:jest": "npm run test:jest -- --watch --collectCoverage=false",
    "watch:prod": "npm run build:prod -- --watch",
    "webdriver:update": "cross-env webdriver-manager update",
    "webdriver:start": "cross-env webdriver-manager start",
    "webpack": "cross-env BABEL_ENV=node ./node_modules/.bin/webpack",
    "webpack-dev-server": "cross-env BABEL_ENV=node ./node_modules/.bin/webpack-dev-server"
  },
  "dependencies": {
    "aurelia-bootstrapper": "2.1.1",
    "aurelia-configuration": "^1.0.17",
    "aurelia-dialog": "^1.0.0-rc.1.0.3",
    "aurelia-framework": "1.1.2",
    "aurelia-google-maps": "^1.0.21",
    "aurelia-history-browser": "1.0.0",
    "aurelia-http-client": "^1.1.1",
    "aurelia-loader-webpack": "2.1.0",
    "aurelia-logging-console": "1.0.0",
    "aurelia-pal-browser": "1.2.1",
    "aurelia-polyfills": "1.2.1",
    "aurelia-router": "1.3.0",
    "aurelia-templating": "1.4.2",
    "aurelia-templating-binding": "1.3.0",
    "aurelia-templating-resources": "1.4.0",
    "aurelia-templating-router": "1.1.0",
    "bluebird": "^3.5.0",
    "blueimp-md5": "^2.7.0",
    "chart.js": "2.4.0",
    "js-cookie": "^2.1.3",
    "moment": "^2.17.1",
    "nprogress": "^0.2.0"
  },
  "devDependencies": {
    "@types/jest": "19.2.3",
    "@types/lodash": "4.14.64",
    "@types/node": "7.0.18",
    "@types/nprogress": "0.0.29",
    "@types/webpack": "2.2.15",
    "aurelia-loader-nodejs": "1.0.1",
    "aurelia-pal-nodejs": "1.0.0-beta.1.0.0",
    "aurelia-protractor-plugin": "1.0.3",
    "aurelia-testing": "1.0.0-beta.3.0.1",
    "aurelia-tools": "^1.0.0",
    "aurelia-webpack-plugin": "2.0.0-rc.2",
    "autoprefixer": "7.0.0",
    "awesome-typescript-loader": "3.1.3",
    "babel-eslint": "^7.2.3",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "4.0.0",
    "css-loader": "0.28.1",
    "del-cli": "^0.2.0",
    "eslint": "^3.17.1",
    "expose-loader": "0.7.3",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "html-webpack-plugin": "2.28.0",
    "http-server": "0.10.0",
    "husky": "^0.14.1",
    "istanbul-instrumenter-loader": "2.0.0",
    "jasmine-core": "2.6.1",
    "jest": "20.0.0",
    "jest-cli": "~20.0.0",
    "json-loader": "0.5.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "lesshint": "^3.1.0",
    "node-sass": "^4.5.3",
    "normalize.css": "^5.0.0",
    "nps": "5.1.0",
    "nps-utils": "1.2.0",
    "postcss-loader": "1.3.3",
    "protractor": "5.1.1",
    "retire": "^1.2.11",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.6",
    "standard-version": "^4.2.0",
    "style-loader": "0.17.0",
    "ts-jest": "20.0.0",
    "ts-node": "3.0.4",
    "tslint": "^4.4.2",
    "typescript": "2.3.2",
    "url-loader": "0.5.8",
    "validate-commit-msg": "^2.12.2",
    "wait-on": "2.0.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "2.4.5"
  }
}
