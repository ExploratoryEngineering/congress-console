{
  "name": "lora-self-service-portal",
  "version": "1.2.5",
  "description": "Front end for self service portal for Exploratory Engineering LORA",
  "main": "dist",
  "productName": "LORA Self Service Portal",
  "bundlesize": [
    {
      "path": "./dist/app.*.js",
      "maxSize": "175 kB"
    },
    {
      "path": "./dist/*chunk*.js",
      "maxSize": "75 kB"
    }
  ],
  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "cross-env NODE_ENV=development npm run webpack -- --progress --profile",
    "build:prod": "webpack -p --env.production --env.extractCss --verbose false",
    "clean": "npm cache clean && del-cli node_modules \"test/coverage\" dist",
    "clean:dist": "del-cli dist",
    "clean:install": "npm install",
    "clean:start": "npm start",
    "commitmsg": "validate-commit-msg",
    "deploy:prod": "npm run test && npm run build:prod && ./scripts/deployToProd.sh",
    "dev": "npm run server",
    "fix": "npm run fix:stylelint",
    "fix:stylelint": "npm run test:stylelint -- --fix",
    "preclean:start": "npm run clean",
    "pree2e": "npm run webdriver:update -- --standalone",
    "prebuild:dev": "npm run clean:dist",
    "prebuild:prod": "npm run clean:dist",
    "preclean:install": "npm run clean",
    "release": "standard-version --silent",
    "release:as": "npm run release -- --release-as",
    "release:patch": "npm run release -- --release-as patch",
    "release:minor": "npm run release -- --release-as minor",
    "release:major": "npm run release -- --release-as major",
    "server": "npm run server:dev:hmr --",
    "server:dev": "webpack-dev-server -d --devtool '#source-map' --inline --env.server --https",
    "server:dev:hmr": "npm run server:dev -- --hot",
    "server:prod": "npm run build:prod && http-server dist --cors -p 8080 --silent",
    "start": "npm run server --",
    "test": "npm run test:jest && npm run test:lint && npm run test:stylelint && npm run test:retire && npm run test:build",
    "test:travis": "npm run test:jest && npm run test:lint && npm run test:stylelint && npm run test:retire && npm run build:prod && npm run test:bundlesize",
    "test:build": "npm run build:prod && npm run test:bundlesize",
    "test:bundlesize": "bundlesize",
    "test:jest": "del-cli test/coverage-jest && jest --config jest.config.json --forceExit",
    "test:eslint": "eslint src test/unit",
    "test:lint": "npm run test:eslint && npm run test:tslint",
    "test:stylelint": "stylelint 'src/**/*.scss' 'styles/**/*.scss'",
    "test:tslint": "tslint src/**/*.ts -c tslint.json && tslint test/**/*.ts -c tslint.json",
    "test:retire": "retire . --ignore .chrome",
    "test:debug": "npm test -- --single-run=false --debug",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:test:jest": "npm run test:jest -- --watch --collectCoverage=false",
    "watch:prod": "npm run build:prod -- --watch",
    "webdriver:update": "cross-env webdriver-manager update",
    "webdriver:start": "cross-env webdriver-manager start",
    "webpack": "cross-env BABEL_ENV=node ./node_modules/.bin/webpack",
    "webpack-dev-server": "cross-env BABEL_ENV=node ./node_modules/.bin/webpack-dev-server"
  },
  "dependencies": {
    "@exploratoryengineering/data-mapper-chain": "^0.5.2",
    "@telenorfrontend/tn-components": "^0.1.2",
    "aurelia-bootstrapper": "2.1.1",
    "aurelia-dialog": "2.0.0-rc.3",
    "aurelia-framework": "^1.3.0",
    "aurelia-google-maps": "2.2.1",
    "aurelia-http-client": "1.3.0",
    "aurelia-polyfills": "^1.3.0",
    "aurelia-templating": "^1.8.2",
    "bluebird": "3.5.1",
    "blueimp-md5": "2.10.0",
    "chart.js": "^2.7.2",
    "js-cookie": "2.2.0",
    "moment": "^2.21.0",
    "nprogress": "0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@telenorfrontend/eslint-config-telenor": "^0.1.3",
    "@telenorfrontend/stylelint-config-telenor": "^0.1.3",
    "@telenorfrontend/tslint-config-telenor": "^0.1.0",
    "@types/chart.js": "^2.7.22",
    "@types/jest": "^23.3.10",
    "@types/lodash": "4.14.104",
    "@types/nprogress": "0.0.29",
    "@types/webpack": "^4.4.21",
    "aurelia-loader-nodejs": "1.0.1",
    "aurelia-pal-nodejs": "1.2.0",
    "aurelia-testing": "1.0.0",
    "aurelia-webpack-plugin": "3.0.0",
    "autoprefixer": "7.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^23.6.0",
    "bundlesize": "^0.17.0",
    "copy-webpack-plugin": "4.6.0",
    "cross-env": "5.0.1",
    "css-loader": "0.28.4",
    "del-cli": "^1.1.0",
    "eslint": "^4.19.1",
    "expose-loader": "0.7.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "3.2.0",
    "http-server": "^0.11.1",
    "husky": "^0.14.3",
    "istanbul-instrumenter-loader": "3.0.0",
    "jasmine-core": "2.8.0",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jest-junit-reporter": "^1.1.0",
    "json-loader": "0.5.7",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "normalize.css": "^7.0.0",
    "nps": "5.7.0",
    "nps-utils": "1.3.0",
    "postcss-loader": "2.0.6",
    "retire": "^1.3.3",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.6",
    "standard-version": "^4.4.0",
    "style-loader": "0.18.2",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.1.0",
    "stylelint-order": "^0.8.1",
    "stylelint-scss": "^2.4.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.1",
    "ts-node": "^3.3.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.8.0",
    "typescript": "^2.7.2",
    "uglify-js": "^3.0.27",
    "url-loader": "1.1.2",
    "validate-commit-msg": "^2.14.0",
    "wait-on": "^2.1.0",
    "webpack": "4.28.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "3.1.10"
  }
}
